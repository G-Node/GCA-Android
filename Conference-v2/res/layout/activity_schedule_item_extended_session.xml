<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fbffb5"
    tools:context=".ScheduleItemExtended" >

    <TextView
        android:id="@+id/schedule_session_title"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
		android:textColor="#3e720b"
		android:background="#d6de50"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Large"
        android:text="@string/hello_world" />

	<TextView
        android:id="@+id/schedule_session_subtitle"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
		android:textColor="#0a5690"
		android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium"
        android:layout_below="@+id/schedule_session_title"
        android:text="@string/hello_world" />

	
	<TableLayout 
	    android:id="@+id/schedule_session_track_detail_table"
	    android:layout_below="@+id/schedule_session_subtitle"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:paddingLeft="15dp"
	    android:stretchColumns="1">	
	    	
    </TableLayout>
	
    
</RelativeLayout>
